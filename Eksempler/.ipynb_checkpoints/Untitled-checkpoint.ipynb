    "import math\n",
    "def formel(x):\n",
    "    return 1/3 * x**3  + 1/2 * x**2 + 1/6*x\n",
    "\n",
    "for x in range(1, 100):\n",
    "    if math.sqrt(formel(x))==0:\n",
    "        print(x)\n",
    "    \n"
